{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/*.ts": {
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}